	WHEN	Gender	=	'M'	AND	Age	IN	('16','17')	AND	AssName	=	'RCADS (Revised Childrens Anxiety and Depression Scale) - Parent version - Separation Anxiety Score'	THEN	((RawScore	- 1.15)*10)/1.55 + 50
